ul{list-style: none;}
li{list-style-type: none;padding-left: 15px;margin: 0;}
a{text-decoration: none; color: #000;}
a:hover{color: #ff6d00;}
a:-webkit-any-link{cursor: auto;}
div{display: block;}
input{margin:0px;font:inherit;}
/*ҳͷ*/
.header{position: absolute;top:0px;left:0px;height: 40px;width: 100%;background-color: #f8f8f8;}
.header_top{font-size: 13px;margin-top:11px;}
.header_top li{float: left;}
.header_search{position: absolute;left:420px;top:100px;width:650px;}
.header_search input{font-family:cursive;text-indent:1em;color:#929292;outline:none;border-radius: 50px;width: 400px;height: 35px;}
.header_bottom{width: 100%;height: 70px;position:absolute;top: 194px;display: block;}
.container {margin: auto 110px;width:1050px;border-bottom: 1px solid #f8f8f8;}
.container div{float: left;}
.h_menu8 {padding: 0 1em;padding: 5px 5px;color: #000;font-size: 1.4em;font-weight: 400;margin: 0.8em 0 0;}
.dropdown-menu ul{border-left: 10px;}
.dropdown-menu li{padding: 0;}
.dropdown-menu li a{width: 176px;line-height:40px;border-radius: 0px;}
.dropdown-menu li a:hover{padding-left:25px;margin: 0;border: 0;}
.btn{font-size: 16px;width: 150px;height: 40px;}

.products{margin:7px 50px;}
.products span{float: left; font-size: 15px;padding-left:50px;}
.pictures{position: absolute;top: 236px;margin-left: 140px;}
.container-fluid{position: absolute;top: 600px;margin-left: 60px;}
.container-fluid li{width: 335px;float: left;}
.container-fluid div{height:300px;}
